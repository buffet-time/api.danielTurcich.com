{
	"name": "danielturichdotcomapi",
	"version": "1.0.0",
	"private": false,
	"description": "Monorepo containing my website and discord bots APIs",
	"author": {
		"email": "packagejson@danielTurcich.com",
		"name": "Daniel Turcich",
		"url": "https://danielTurcich.com"
	},
	"type": "module",
	"engines": {
		"node": "^19.4.0",
		"pnpm": "^8.1.0"
	},
	"scripts": {
		"bot:serve": "cd ./src/buffetBotJsApi/ && pnpm serve",
		"chart:serve": "cd ./src/musicChartApi/ && pnpm serve",
		"website:serve": "cd ./src/danielTurcichComApi/ && pnpm serve",
		"lint-check": "eslint \"**/**/*.ts\" && echo lint-check done",
		"format-check": "prettier --check \"**/**\" && echo format-check done",
		"bot:build": "cd ./src/buffetBotJsApi/ && pnpm build",
		"chart:build": "cd ./src/musicChartApi/ && pnpm build",
		"website:build": "cd ./src/danielTurcichComApi/ && pnpm build",
		"build-checks": "pnpm bot:build && pnpm chart:build && pnpm website:build",
		"checks": "pnpm lint-check && pnpm format-check && pnpm build-checks",
		"prepare": "husky install"
	},
	"dependencies": {
		"@google-cloud/local-auth": "^2.1.1",
		"@octokit/request": "^6.2.3",
		"fastify": "^4.15.0",
		"fastify-cors": "^6.1.0",
		"file-system": "^2.2.2",
		"google-auth-library": "8.6.0",
		"googleapis": "108.0.1",
		"snoowrap": "^1.23.0"
	},
	"devDependencies": {
		"@octokit/types": "^8.2.1",
		"@types/node": "^18.15.11",
		"@types/request": "^2.48.8",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"googleapis-common": "^6.0.4",
		"husky": "^8.0.3",
		"parcel": "^2.8.3",
		"prettier": "^2.8.7",
		"typescript": "^5.0.3",
		"undici": "^5.21.0"
	}
}
