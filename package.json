{
	"name": "danielturichdotcomapi",
	"version": "1.0.0",
	"private": false,
	"description": "API for danielTurcich.com",
	"author": {
		"email": "packagejson@danielTurcich.com",
		"name": "Daniel Turcich",
		"url": "https://danielTurcich.com"
	},
	"type": "module",
	"engines": {
		"node": "^20.0.0",
		"pnpm": "^8.1.0"
	},
	"scripts": {
		"build": "bun build ./src/main.ts --outdir ./src/dist",
		"website:serve": "cd ./src/ && bun serve",
		"lint-check-1": "eslint \"**/**/*.ts\" && echo lint-check done",
		"lint-check": "echo re-implement",
		"format-check-1": "prettier --check \"**/**\" && echo format-check done",
		"format-check": "echo re-implement",
		"website:build": "cd ./src/ && bun build",
		"build-checks": "pnpm bot:build && bun website:build",
		"checks": "bun lint-check && bun format-check && bun build-checks",
		"prepare": "husky install"
	},
	"dependencies": {
		"@google-cloud/local-auth": "^2.1.1",
		"fastify": "^4.21.0",
		"fastify-cors": "^6.1.0",
		"file-system": "^2.2.2",
		"google-auth-library": "^8.9.0",
		"googleapis": "^122.0.0"
	},
	"devDependencies": {
		"@types/node": "^20.5.6",
		"@types/request": "^2.48.8",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"googleapis-common": "^6.0.4",
		"husky": "^8.0.3",
		"prettier": "^3.0.2",
		"typescript": "5.0.4",
		"undici": "^5.23.0"
	}
}
